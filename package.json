{
	"type": "module",
	"dependencies": {
		"discord.js-selfbot-v13": "^3.4.6",
		"dotenv": "^16.4.7",
		"prettier": "^3.4.2",
		"typescript": "^5.7.2"
	},
	"scripts": {
		"fmt": "prettier --log-level=warn --write './*.ts'",
		"build": "tsc",
		"start": "bash -c 'node . 2>&1 >log & echo $! >pid'",
		"stop": "bash -c 'kill $(<pid)'"
	},
	"prettier": {
		"arrowParens": "avoid",
		"trailingComma": "none",
		"useTabs": true,
		"printWidth": 150,
		"tabWidth": 4,
		"semi": true,
		"singleQuote": true,
		"proseWrap": "preserve"
	}
}